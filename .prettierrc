{
  "semi": true,
  "arrowParens": "avoid",
  "quoteProps": "consistent",
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "svelteSortOrder": "options-scripts-markup-styles",
  "svelteAllowShorthand": true,
  "svelteBracketNewLine": true,
  "svelteIndentScriptAndStyle": false,
  "printWidth": 80,
  "plugins": [
    "prettier-plugin-svelte"
  ]
}